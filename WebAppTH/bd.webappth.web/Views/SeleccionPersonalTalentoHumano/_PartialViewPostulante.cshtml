@model bd.webappth.entidades.ViewModels.ViewModelSeleccionPersonal
<fieldset>
    <div class="row">
        <section class="col col-lg-12">
            <label class="input">
                @Html.DisplayNameFor(model => model.UnidadAdministrativa):
                @Html.DisplayFor(model => model.UnidadAdministrativa)
            </label>
        </section>
    </div>
    <div class="row">
        <section class="col col-lg-12">
            <label class="input">
                @Html.DisplayNameFor(model => model.PuestoInstitucional):
                @Html.DisplayFor(model => model.PuestoInstitucional)
            </label>
        </section>
    </div>
    <div class="row">
                        <table id="datatable_fixed_column" class="table table-striped table-bordered table-hover" width="100%">
                            <thead>
                                <tr>
                                    <th>Nombres</th>
                                    <th>Cedula</th>
                                    <th>Experiencia en Dias</th>
                                    <th>Experiencia / Años/Meses/Dias</th>
                                    <th>Opciones</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.ListasCanditadoExperiencia)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Nombres)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Cedula)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ExperienciaDias)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ExperienciaMesAno)
                                        </td>
                                        

                                        <td>
                                           
                                                @Html.ActionLink("Evaluar", "Create", new { id = item.Cedula })
                                            
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                  </div>
</fieldset>