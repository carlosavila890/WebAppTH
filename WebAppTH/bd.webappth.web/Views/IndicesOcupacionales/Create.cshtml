                                                            <section class="col col-6">
                                                                <label class="input">
                                                                    <select asp-for="IndiceOcupacional.IdDependencia" asp- class="form-control" asp-items="ViewBag.IdDependencia">
                                                                        <option value="0" selected="" disabled="">@Html.DisplayNameFor(model => model.IndiceOcupacional.IdDependencia)</option>
                                                                    </select>
                                                                    <span asp-validation-for="IndiceOcupacional.IdDependencia" class="text-danger"></span>
                                                                </label>
                                                            </section>
                                                            <section class="col col-6">
