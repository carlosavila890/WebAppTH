                                                                        <option value="0" selected="" disabled="">@Html.DisplayNameFor(model => model.IndiceOcupacional.IdEscalaGrados)</option>
                                                                    </select>
                                                                    <span asp-validation-for="IndiceOcupacional.IdEscalaGrados" class="text-danger"></span>
                                                                </label>
                                                            </section>
                                                        </div>
                                                    </fieldset>
                                                    <footer>
                                                        <input type="submit" value="Guardar" class="btn btn-primary" />
                                                        <a asp-action="Index" class="btn btn-default">Cancelar</a>
                                                    </footer>
                                                </form>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                                <div class="step-pane" id="step2">
