@model bd.webappth.entidades.Negocio.PartidasFase
<fieldset>
    <div class="row">
        <section class="col col-lg-6">
            <label>Tipo Concurso</label>
            <label class="input">
                <select asp-for="IdTipoConcurso" class="form-control" id="IdTipoConcurso" asp-items="ViewBag.IdTipoConcurso">
                    <option value="0" selected="selected" disabled=""></option>
                </select>
                <span asp-validation-for="IdTipoConcurso" class="text-danger"></span>
            </label>
        </section>
        <section class="col col-lg-6">
            <label>Puesto Vacantes</label>
            <label class="input">
                <select asp-for="IdIndiceOcupacional" class="form-control" id="IdIndiceOcupacional" asp-items="ViewBag.IdIndiceOcupacional">
                    <option value="0" selected="selected" disabled=""></option>
                </select>
                <span asp-validation-for="IdIndiceOcupacional" class="text-danger"></span>
            </label>

        </section>
    </div>

