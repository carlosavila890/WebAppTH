@model bd.webappth.entidades.Negocio.AvanceGestionCambio
<fieldset>
     <div class="row">
        <section class="col col-8">
            <label class="input">
                <i class="icon-append fa fa-calendar"></i>
                <input maxlength="20" asp-for="Fecha" class="datepicker" data-dateformat='dd/mm/yy' data-mask="99/99/9999" placeholder="@Html.DisplayNameFor(model=>model.Fecha)" />
                <span asp-validation-for="Fecha" class="text-danger"></span>
            </label>
        </section>
    </div>
    <div class="row">
        <section class="col col-8">
            <label class="input">
                <i class="icon-append fa fa-check"></i>
                <input maxlength="9" asp-for="Indicadorreal"  onKeyPress="return checkIt(event)"  placeholder="@Html.DisplayNameFor(model=>model.Indicadorreal)" />
                <span asp-validation-for="Indicadorreal" class="text-danger"></span>
            </label>
        </section>
    </div>
</fieldset>